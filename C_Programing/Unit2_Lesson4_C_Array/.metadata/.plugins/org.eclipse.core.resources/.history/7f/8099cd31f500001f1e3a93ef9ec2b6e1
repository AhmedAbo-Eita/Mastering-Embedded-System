#include<stdio.h>
#include<string.h>

int a[];
int counter,flag = 0;// for loop counter
char input;

int main()
{
	printf("Enter a string :");
	fflush(stdin);fflush(stdout);
	gets(a);





	printf("Enter a charaacter to find frequency :");
	fflush(stdin);fflush(stdout);
	scanf("%s", &input);


	for(counter = 0 ; counter <= strlen(a); counter++)
	{
		if(a[counter] == input )
		{
			flag = flag +1;
		}
	}

	printf("frequency of %c  = %d", input, flag);



	return 0;
}

